<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="minTextSize" format="dimension" />

    <declare-styleable name="AutoFitTextEditText">
        <attr name="minTextSize" />
        <attr name="maxTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="AutofitTextView">
        <attr name="minTextSize" />
        <attr name="precision" format="float" />
        <attr name="sizeToFit" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TextViewWithStroke">
        <attr name="stroke_color" format="reference" />
        <attr name="stroke_width" format="integer" />
    </declare-styleable>

    <declare-styleable name="TextViewPlus">
        <attr name="customFont" format="string" />
    </declare-styleable>

</resources>